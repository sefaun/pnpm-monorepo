{
  "name": "sefa-fesa",
  "version": "1.1.8",
  "description": "sefa CLI Application",
  "author": "sefa",
  "license": "ISC",
  "type": "module",
  "packageManager": "pnpm@9.9.0",
  "main": "/build/index.js",
  "types": "/build/index.d.ts",
  "bin": {
    "sefa-fesa": "./bin/run.js"
  },
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "@oclif/core": "4.0.21",
    "@sefa-fesa/first": "1.0.7",
    "@sefa-fesa/last": "1.0.9"
  },
  "devDependencies": {
    "@types/node": "^22.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "homepage": "https://github.com/sefaun/pnpm-monorepo/tree/master/packages/cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sefaun/pnpm-monorepo.git"
  },
  "engines": {
    "node": ">= 18"
  },
  "oclif": {
    "bin": "sefa-fesa",
    "dirname": "sefa-fesa",
    "commands": {
      "strategy": "explicit",
      "target": "./build/index.js",
      "identifier": "COMMANDS"
    },
    "topicSeparator": " ",
    "topics": {
      "first": {
        "description": "First işlemler"
      },
      "last": {
        "description": "Last işlemler"
      }
    }
  },
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  }
}